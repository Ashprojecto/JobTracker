// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}



model Job {
  id           String   @id @default(cuid())
  company      String
  position     String
  stage        String
  dateApplied  String
  createdAt    DateTime @default(now())
}